    Assert( Reserved == 0 );
} TChunkHeader;

typedef struct
{
    u16 Length;
    char Str[Length];  
} TString <optimize=false>;

typedef struct(u32 version)
{
    u16 Length;
    if ( Length )
    {
        char Str[Length];  
    
        if ( version >= 0x01105100 )
            SeekCurrent( 1 ); // padding byte
        
        if ( version > 0x1080000 )
        {
            // hash
            u32 Hash;
        }   
    }  
} THashString <optimize=false>;

typedef struct(u32 version)
{
    u16 Length;
    if ( Length )
    {
        char Str[Length];  
    
        if ( version > 0x1080000 )
        {
            // hash
            u32 Hash;
        }    
    } 
} THashStringNoPad <optimize=false>;

